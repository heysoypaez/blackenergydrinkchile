<!-- Qty number script -->
<script>
  jQuery('<div class="quantity-nav"><div class="quantity-button quantity-up">+</div><div class="quantity-button quantity-down">-</div></div>').insertAfter('.quantity input');
  jQuery('.quantity.quantity-3167678').each(function() {
    var spinner = jQuery(this),
        input = spinner.find('input[type="number"]'),
        btnUp = spinner.find('.quantity-up'),
        btnDown = spinner.find('.quantity-down'),
        min = input.attr('min'),
        max = input.attr('max');

    btnUp.click(function() {
      var oldValue = parseFloat(input.val());
      if (oldValue >= max) {
        var newVal = oldValue;
      } else {
        var newVal = oldValue + 1;
      }
      spinner.find("input").val(newVal);
      spinner.find("input").trigger("change");
    });

    btnDown.click(function() {
      var oldValue = parseFloat(input.val());
      if (oldValue <= min) {
        var newVal = oldValue;
      } else {
        var newVal = oldValue - 1;
      }
      if(newVal > 0){
        spinner.find("input").val(newVal);
        spinner.find("input").trigger("change");
      };
    });

  });
</script>

<script>
  
  function addToCart2(id, productName, qty){
    Jumpseller.addProductToCart(id, qty, {}, {callback: function(data, status){
      toastr.options = {
        "closeButton": true,
        "debug": false,
        "newestOnTop": false,
        "progressBar": true,
        "positionClass": "toast-top-right",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "500",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
      }  

      if(parseInt(qty) == 1){
        toastr.success(qty + ' "' + productName + '" Se ha agregado al carro de compras. <a href="/cart"> Ir a Carro de Compras.</a>');
      } else {
        toastr.success(qty + ' "' + productName + '" Se han agregado al carro de compras. <a href="/cart"> Ir a Carro de Compras.</a>');
      }
      //setTimeout(function(){location.reload();}, 2000);

      // update cart in page
      $(".cart-size").text(parseInt($(".cart-size").text()) + parseInt(qty));
      $('.order-total').text(data.total);
    }
                                             });
  }
</script>

    </div>
    
